<div class="posts">
  <div class="container">
    <h2><%= link_to "New Post", "posts/new" %></h2>
    <br>
    <% @posts.each do |post|%> 
      <div class="post">
        <h3><%= link_to post.title, post_path(post) %></h3> 
        <h4><%= post.location %></h4>
        <br>
      </div> 
    <% end %>
  </div>
</div>